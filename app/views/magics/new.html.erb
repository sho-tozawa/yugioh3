<%= form_with(model: @magic, local: true) do |form| %>

  <div class="name">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="magic-effect">
    <%= form.label :effect %>
    <textarea><% form.text_field :effect %></textarea>
  </div>


  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>